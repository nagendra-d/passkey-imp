{
  "version": 2,
  "builds": [
    {
      "src": "api/aasa.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ],
  "headers": [{
    "source": "/.well-known/apple-app-site-association",
    "headers": [{
      "key": "Content-Type",
      "value": "application/json"
    }]
  }]
}
