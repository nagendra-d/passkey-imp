{
  "version": 2,
  "builds": [
    {
      "src": "api/aasa.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    },
    {
      "src": "/\\.well-known/apple-app-site-association$",
      "dest": "/api/apple-app-site-association.js"
    }
  ]
}
