{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    },
    {
      "src": "/\\.well-known/apple-app-site-association",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}
